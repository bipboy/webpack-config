{
  "name": "@bipboys/webpack-config",
  "version": "0.0.6",
  "description": "Webpack build config for react ssr apps (from scratch)",
  "author": "Bipboys Dev Team",
  "sideEffects": false,
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "types/index.d.ts",
  "files": [
    "build/*",
    "types/*",
    "!*.tsbuildinfo",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npm run build:clean && swc ./src -d build && npm run build:types",
    "build:clean": "rimraf ./build && mkdir build",
    "build:types": "tsc -p tsconfig.json",
    "clean": "rimraf ./node_modules && npm run build:clean"
  },
  "dependencies": {
    "@bipboys/babel-preset": "0.0.2",
    "@bipboys/typescript-preset": "0.0.1",
    "@bipboys/webpack-polyfill-plugin": "0.1.3",
    "@bipboys/webpack-stats-plugin": "^0.4.2",
    "babel-loader": "^8.2.5",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.17",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "react-dev-utils": "^12.0.1",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "^9.4.1",
    "tslib": "^2.4.0",
    "typed-css-modules-webpack-plugin": "^0.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.7",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.4",
    "@types/webpack-env": "^1.18.0"
  }
}
